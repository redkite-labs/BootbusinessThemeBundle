    "alphalemon/app-bootstrap-label-bundle": "dev-master",
    "alphalemon/app-bootstrap-slider-bundle": "dev-master",
    "alphalemon/app-bootstrap-thumbnail-bundle": "dev-master",
    "alphalemon/app-bootstrap-navbar-bundle": "dev-master",
    "alphalemon/app-bootstrap-accordion-bundle": "dev-master"
